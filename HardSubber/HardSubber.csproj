    <PropertyGroup>
        <RuntimeIdentifiers>linux-x64;linux-musl-x64;linux-arm;linux-arm64;win-x64;win-x86;win-arm;win-arm64</RuntimeIdentifiers>
    </PropertyGroup>
    
</Project>
